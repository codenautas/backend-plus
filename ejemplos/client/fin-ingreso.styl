body
  font-family arial
  background-color white

#fin-ingreso
  font-family arial
  font-size 300%
  
@media (max-width: 480px)
  #logo-encuesta
    width 480px
    margin 0 0 20px 0
  
@media (min-width: 480px) and (max-width: 768px)
  #logo-encuesta
    width 100%
    margin 0 0 50px 0

@media (min-width: 768px) and (max-width: 1152px)
  #logo-encuesta
    width 768px
    margin 0 0 50px 0

@media (min-width: 1152px)
  #logo-encuesta
    width 67%
    margin 0 0 50px 0
